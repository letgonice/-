/* packageA/pages/supportval/supportval.wxss */
.supportVal{
  height: 1460rpx;
  position: relative;
}
.content{
  margin: 0 auto;
  width: 694rpx;
  margin-bottom: 140rpx;
}
.content .goodsSupportVal{
  box-sizing: border-box;
  margin-top: 42rpx;
  padding-top: 32rpx;
  /* padding-left: 38rpx; */
  height: 296rpx;
  box-shadow: 0px 2px 10px 0px rgba(0, 0, 0, 0.4);
}
.content .SupportValText{
  display: flex;
  margin:24rpx 0;
  align-items: center;
  font-size: 36rpx;
  margin-left: 28rpx;
}
.content .orderNumber{
  margin-left: 38rpx;
}
.content .SupportValText .goodsSupportImg{
  width: 56rpx;
  height: 56rpx;
  margin-right: 20rpx;
}
.content .SupportValText .goodsSupportImg image{
  width: 100%;
  height:100%;
}


.SupportValinput{
  margin: 0 auto;
  width: 568rpx;
  height: 68rpx;
  border: 2rpx solid #ccc;
  border-radius: 20rpx;
  padding-left: 20rpx;
}

.insurance{
  margin-top: 50rpx;
  overflow: hidden;
  box-shadow: 0px 2px 10px 0px rgba(0, 0, 0, 0.4);
}
.insurance .warnImg{
  width: 28rpx;
  height: 28rpx;
  padding-right: 10rpx;
}
.insurance .warnImg image{
  width: 100%;
  height: 100%;
}
.insurance .warnContent{
  display: flex;
  font-size: 22rpx;
  color:#EC0606;
  margin:26rpx 0 0 26rpx;
}
 .switch{
margin-top: 14rpx;
}
.insurance .van-cell__title{
  display: flex;
  align-items: center;
}

.endButton {
  position:absolute;
  bottom: 0;
  display: flex;
  width: 100%;
}

.shade{
  position: absolute;
  top: 0;
  bottom: 0;
  left: 0;
  right: 0;
  background-color:rgba(0, 0, 0, 0.4);
  z-index: 999;
}
.shade .riskInform{
  width: 632rpx;
  /* height: 446rpx; */
  height: 500rpx;
  background-color: white;
  border-radius: 20rpx;
}
.shade .riskInform .title{
  padding-top: 20rpx;
  text-align: center;
  font-size: 36rpx;
}
.shade .riskInform .riskContent{
  box-sizing: border-box;
  padding: 0 16rpx;
  font-size: 28rpx;
  line-height: 50rpx;
}
.riskBtn{
  display: flex;
  padding: 50rpx 32rpx;
  justify-content: space-between;
}
.riskBtn .van-button{
  width: 236rpx;
  height: 66rpx;
  border: none;
  border-radius: 10rpx;
}
.riskBtn .leftBtn .van-button{
  background-color: #274f05;
  color: #ffd700;
}
.riskBtn .rightBtn .van-button{
  background-color:#4c168e;
  color: white;
}
.riskText{
  padding-left: 40rpx;
}


