/* components/endButton/endButton.wxss */
.endButton{
  display: flex;
  width: 100%;
  height: 114rpx;
}
.leftCall, .rightNext{
  width: 50%;
}
.endButton .van-button{
  border: none;
  width: 100%;
  height: 100%;
  color: white;
  font-size: 36rpx;
}

.leftCall .van-button{
  background-color: #4c168e;
}
.rightNext .van-button{
  background-color: #274f05;
  color:#F5E4AD;
}